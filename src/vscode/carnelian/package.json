{
    "name": "carnelian",
    "displayName": "carnelian-syntax",
    "description": "carnelian syntax highlighter",
    "version": "0.0.1",
    "publisher": "mrange",
    "engines": {
        "vscode": "^1.16.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
